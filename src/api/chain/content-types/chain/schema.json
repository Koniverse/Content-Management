{
  "kind": "collectionType",
  "collectionName": "chains",
  "info": {
    "singularName": "chain",
    "pluralName": "chains",
    "displayName": "Chain",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ordinal": {
      "type": "integer",
      "default": 9999
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "name": {
      "type": "string"
    },
    "isTestnet": {
      "type": "boolean"
    },
    "chainStatus": {
      "type": "enumeration",
      "enum": [
        "ACTIVE",
        "INACTIVE",
        "STOPPED"
      ],
      "default": "ACTIVE"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "providers": {
      "type": "component",
      "repeatable": true,
      "component": "chain-info.provider"
    },
    "substrateInfo": {
      "type": "component",
      "repeatable": false,
      "component": "chain-info.subtrate-info"
    },
    "evmInfo": {
      "type": "component",
      "repeatable": false,
      "component": "chain-info.evm-info"
    },
    "crowdloanFunds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::crowdloan-fund.crowdloan-fund",
      "mappedBy": "chain"
    },
    "extraInfo": {
      "type": "component",
      "repeatable": false,
      "component": "chain-info.extra-info"
    },
    "bitcoinInfo": {
      "type": "component",
      "repeatable": false,
      "component": "chain-info.bitcoin-info"
    }
  }
}
