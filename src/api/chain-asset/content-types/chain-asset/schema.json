{
  "kind": "collectionType",
  "collectionName": "chain_assets",
  "info": {
    "singularName": "chain-asset",
    "pluralName": "chain-assets",
    "displayName": "Chain Asset",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "ordinal": {
      "type": "integer",
      "default": 9999
    },
    "originChain": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::chain.chain",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "symbol": {
      "type": "string",
      "required": true
    },
    "decimals": {
      "type": "integer"
    },
    "priceId": {
      "type": "string"
    },
    "minAmount": {
      "type": "string"
    },
    "assetType": {
      "type": "enumeration",
      "enum": [
        "NATIVE",
        "LOCAL",
        "ERC20",
        "ERC721",
        "PSP22",
        "PSP34",
        "GRC20",
        "GRC721",
        "RUNE",
        "BRC20",
        "UNKNOWN"
      ],
      "required": true
    },
    "metadata": {
      "type": "json"
    },
    "hasValue": {
      "type": "boolean"
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "multiChainAsset": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::multi-chain-asset.multi-chain-asset",
      "inversedBy": "chainAssets"
    },
    "assetRefs": {
      "type": "component",
      "repeatable": true,
      "component": "asset-info.asset-ref"
    }
  }
}
