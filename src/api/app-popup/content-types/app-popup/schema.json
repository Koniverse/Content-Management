{
  "kind": "collectionType",
  "collectionName": "app_popups",
  "info": {
    "singularName": "app-popup",
    "pluralName": "app-popups",
    "displayName": "App Popup",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "info": {
      "type": "component",
      "repeatable": false,
      "component": "app-content.ac-info",
      "required": true
    },
    "priority": {
      "type": "integer",
      "default": 999
    },
    "position": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "token",
        "nft",
        "earning",
        "crowdloan",
        "dapp",
        "mission_pool",
        "history"
      ]
    },
    "repeat": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "once",
        "every_time",
        "daily",
        "weekly",
        "monthly"
      ],
      "default": "once"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "buttons": {
      "type": "component",
      "repeatable": true,
      "component": "app-content.ac-button"
    },
    "conditions": {
      "type": "dynamiczone",
      "components": [
        "app-content.condition-balance",
        "app-content.condition-crowdloan",
        "app-content.condition-earning",
        "app-content.condition-has-money",
        "app-content.condition-nft"
      ]
    }
  }
}
