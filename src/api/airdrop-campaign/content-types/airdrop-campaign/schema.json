{
  "kind": "collectionType",
  "collectionName": "airdrop_campaigns",
  "info": {
    "singularName": "airdrop-campaign",
    "pluralName": "airdrop-campaigns",
    "displayName": "Airdrop Campaign",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "upcoming",
        "live",
        "claimable",
        "archived"
      ]
    },
    "ordinal": {
      "type": "integer",
      "default": 999
    },
    "description": {
      "type": "text"
    },
    "start_time": {
      "type": "datetime"
    },
    "end_time": {
      "type": "datetime"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "campaign_url": {
      "type": "string",
      "required": true
    },
    "twitter_url": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "backdrop_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "total_supply": {
      "type": "string"
    },
    "reward": {
      "type": "string"
    },
    "total_winner": {
      "type": "string"
    },
    "chains": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chain.chain"
    },
    "tags": {
      "type": "customField",
      "options": [
        "lucky_draw",
        "fcfs",
        "manual_selection",
        "points"
      ],
      "customField": "plugin::multi-select.multi-select"
    }
  }
}
